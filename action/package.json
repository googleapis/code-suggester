  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.10.5",
    "@vercel/ncc": "^0.33.0",
    "c8": "^7.0.1",
    "gts": "^3.0.0",
    "js-yaml": "^4.1.0",
